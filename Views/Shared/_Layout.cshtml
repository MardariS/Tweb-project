@using System.Web.Optimization

<!DOCTYPE html>
<html>
<head>
    <title> Electroland </title>
    <meta charset="utf-8">

    <!-- Meta Files -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="author" content="">
    <meta name="keywords" content="">
    <meta name="description" content="">


    <!-- CSS Bundles -->
    @Styles.Render("~/bundles/css")

    <!-- CSS Links -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@300;400;500&family=Lato:wght@300;400;700&display=swap" rel="stylesheet">

    <!-- Scripts Bundles-->
    @Scripts.Render("~/jquery")
    @Scripts.Render("~/modernizr")
    @Scripts.Render("~/plugins")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/script")

</head>

<body data-bs-spy="scroll" data-bs-target="#navbar" data-bs-root-margin="0px 0px -40%" data-bs-smooth-scroll="true" tabindex="0">
    <!-- Icons -->
    @Html.Partial("_SVG_Icons")

    <!-- Search -->
    @Html.Partial("_Search") <!-- lll -->

    <!-- Header -->
    @Html.Partial("_Header")

    <!-- BillBoard -->
    @Html.Partial("_Billboard")

    <!-- Company Service -->
    @Html.Partial("_CompanyService")

    <!-- RenderBody (Index - Products)-->
    <main>
        @RenderBody()
    </main>

    <!-- Blog -->
    @Html.Partial("~/Views/Shared/Blog/Blog.cshtml")

    <!-- Reviews -->
    @Html.Partial("~/Views/Shared/Reviews/Reviews.cshtml")

    <!-- Subscribe -->
    @Html.Partial("~/Views/Shared/SocialMedia/Subscribe.cshtml")

    <!-- Instagram -->>
    @Html.Partial("~/Views/Shared/SocialMedia/Instagram.cshtml")

    <!-- Footer (Top) -->
    @Html.Partial("~/Views/Shared/Footer/FooterTop.cshtml")

    <!-- Footer (Center) -->
    @Html.Partial("~/Views/Shared/Footer/FooterCenter.cshtml")


    <!-- Script JS (Upside Footer)-->
    @*<script src="js/jquery-1.11.0.min.js"></script>*@
    @Scripts.Render("~/jquery")

    <script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>

    @*<script type="text/javascript" src="js/bootstrap.bundle.min.js"></script>*@
    @Scripts.Render("~/bundles/bootstrap")

    @*<script type="text/javascript" src="js/plugins.js"></script>*@
    @Scripts.Render("~/plugins")

    @*<script type="text/javascript" src="js/script.js"></script>*@
    @Scripts.Render("~/script")

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <!-- Footer (Bottom) -->
    <footer class="bg-dark text-white text-center py-3 mt-5">
        <div class="container">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </div>
    </footer>
</body>

</html>